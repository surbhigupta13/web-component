{
  "name": "ng-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build_pack_button": "ng build && cd dist/button && npm pack",
    "build_pack_alert-bar": "ng build && cd dist/alert-bar && npm pack",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build:elements": "ng build --prod --project elements --output-hashing none && npm run pack:elements && cp projects/elements/package.json dist/elements",
    "pack:elements": "cat ./dist/elements/{runtime,polyfills,main}.js > dist/elements/elements.js && ls -lah dist/elements/elements.js",
    "build:components": "ng build --prod --project components",
    "build-storybook": "build-storybook",
    "dsm-storybook:publish": "dsm-storybook publish",
    "dsm-storybook:preview": "dsm-storybook preview"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^13.0.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@invisionapp/dsm-storybook": "latest",
    "@storybook/addon-controls": "^6.4.8",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addons": "^6.4.8",
    "button": "file:dist/button/button-0.0.1.tgz",
    "document-register-element": "^1.7.2",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.6.0",
    "storybook-addon-web-components-knobs": "^0.3.20",
    "tslib": "^2.3.0",
    "wc-ui-button": "1.0.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/elements": "^12.2.2",
    "@babel/core": "^7.15.0",
    "@compodoc/compodoc": "^1.1.14",
    "@storybook/addon-actions": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/angular": "^6.4.8",
    "@storybook/builder-webpack5": "^6.4.8",
    "@storybook/manager-webpack5": "^6.4.8",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.5.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "babel-loader": "^8.2.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "storybook-addon-angular-ivy": "0.0.1",
    "typescript": "~4.3.5"
  }
}
